{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "3b341102-4bcb-474b-b7cf-09754e762b7d"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@agencycoda/mia-task": {
      "projectType": "library",
      "root": "projects/agencycoda/mia-task",
      "sourceRoot": "projects/agencycoda/mia-task/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/agencycoda/mia-task/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/agencycoda/mia-task/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/agencycoda/mia-task/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/agencycoda/mia-task/src/test.ts",
            "tsConfig": "projects/agencycoda/mia-task/tsconfig.spec.json",
            "karmaConfig": "projects/agencycoda/mia-task/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "@agencycoda/mia-task"
}
